<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">    
	<item android:state_pressed="true">
		<shape>
		    <corners android:radius="13dp"/>
			<solid android:color="#a03d14ec"/>
		</shape>
	</item>
	<item android:state_checked="true">
		<shape>
		    <corners android:radius="13dp"/>
			<solid android:color="#a05f39ff"/>
		</shape>
	</item>
	<item android:state_focused="true">
		<shape>
		    <corners android:radius="13dp"/>
		    <stroke android:width="2dp" android:color="#ffd4d4"/>
			<solid android:color="#a0392492"/>
		</shape>
	</item>
	<item>
	    <shape>
		    <corners android:radius="13dp"/>
			<solid android:color="#a0553dbb"/>
		</shape>
	</item>
</selector>