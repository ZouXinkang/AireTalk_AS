<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#e7724e"/>
        </shape>
    </item>
    <item android:top="1dp" android:bottom="15dp" android:left="4dp" android:right="4dp">
        <shape android:shape="oval">
            <gradient
                android:angle="270"
                android:endColor="#ffe49c"
                android:startColor="#f19e6e"/>
        </shape>
    </item>
	<item>
	    <shape android:shape="oval">
	        <solid android:color="#00000000"/>
	        <stroke android:width="3dp" android:color="#ffffff"/>
	    </shape>
	</item>
</layer-list>